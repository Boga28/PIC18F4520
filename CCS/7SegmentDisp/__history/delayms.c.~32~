#include <delayms.h>   

  
         
void delayMsFun(unsigned int32 msecs)  // Delay function - generates a delay of specified msecs
{                       
    for(int32 i =0 ; i <= msecs; i++)
  {
      for(int32 j = 0; j<2000;j++) nop();  
      
    } 
    //delay_ms(msecs);
}                                     
