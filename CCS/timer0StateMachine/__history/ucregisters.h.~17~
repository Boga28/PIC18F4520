#ifndef UCREGISTER_H
#define UCREGISTER_H
  
#byte TRISD = 0x0F95
#byte PORTD = 0x0F83

#byte TRISB = 0x0F93
#byte PORTB = 0x0F81  

//TIMER0 INTERRUPT
#byte ADCON1 = 0x0FC1  
#byte CMCON  = 0x0FB4
#byte TMR0L  = 0x0FD6  // Low Byte     
#byte T0CON  = 0x0FD5  //TIMER0 
#byte INTCON = 0x0FF2    
#bit test = INTCON.2  


#endif UCREGISTER_H                                                                         
