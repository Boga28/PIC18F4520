#ifndef UCREGISTER_H 
#define UCREGISTER_H  

#byte TRISC   = 0x0F94
#byte PORTC   = 0x0F82 

#bit  UART_TX_DIR = TRISC.0   //TX
#bit  UART_RX_DIR = TRISC.1   //RX
                              
#bit  UART_TX = PORTC.0       //TX   
#bit  UART_RX = PORTC.1       //RX                   
                          
#endif //UCREGISTER_H                     
