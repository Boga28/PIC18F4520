#ifndef ONE_WIRE_H
#define ONE_WIRE_H  


                             
 
                              
/*                        
 * One wire (1-wire) driver for CCS C compiler. Suitable for use with devices
 * such as the DS18B20 1-wire digital temperature sensor.
 */                                                                            
     
void onewire_reset();  
void onewire_write(uchar_t);                                                 
sint8_t onewire_read();       

void OneWireHigh();          
void OneWireRelease();
                        
                       

#endif ONE_WIRE_H
